   "source": [
    "#Minimum and maximum possible sum of any 4 elements in a array\n",
    "arr=[1,2,3,4,5]\n",
    "total=sum(arr)\n",
    "l=[]\n",
    "j=0\n",
    "Min=total\n",
    "Max=0\n",
    "for i in arr:\n",
    "    j=total-i\n",
    "    l.append(j)\n",
    "for i in l:\n",
    "    if Min>i:\n",
    "        Min=i\n",
    "    if Max<i:\n",
    "        Max=i\n",
    "print(f'{Min} {Max}')\n"
   ]
  }
