   "source": [
    "import numpy as np\n",
    "n=int(input())\n",
    "l=list(map(int,input(\"\").split(\" \")))\n",
    "print(np.mean(l))\n",
    "print(np.median(l))\n",
    "from collections import Counter\n",
    "c=Counter(l)\n",
    "c=dict(c)\n",
    "k=max(l)\n",
    "m=max(c.values())\n",
    "for i in c:\n",
    "    if c[i]==m and i<=k:\n",
    "        k=i\n",
    "print(k)"
   ]
