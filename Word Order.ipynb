    "from collections import OrderedDict\n",
    "n=int(input())\n",
    "od=OrderedDict()\n",
    "for i in range(n):\n",
    "    x=input()\n",
    "    if x in od.keys():\n",
    "        od[x]+=1\n",
    "    else:\n",
    "        od[x]=1\n",
    "print(len(od.keys()))\n",
    "for i in od.values():\n",
    "    print(i,end=\" \")"
   ]
  }
