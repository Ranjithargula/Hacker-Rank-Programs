    "S=input()\n",
    "k=int(input())\n",
    "l=len(S)\n",
    "n=l//k\n",
    "import textwrap\n",
    "from collections import Counter\n",
    "L=textwrap.wrap(S,k)\n",
    "for i in L:\n",
    "    d=[]\n",
    "    c=list(i)\n",
    "    for i in c:\n",
    "        if i not in d:\n",
    "            d.append(i)\n",
    "    st=\"\"\n",
    "    for j in d:\n",
    "        st+=j\n",
    "    print(st)"
