   "source": [
    "S=input()\n",
    "L=sorted(list(S))\n",
    "l,u,od,eve=[],[],[],[]\n",
    "for i in L:\n",
    "    if i.islower():\n",
    "        l.append(i)\n",
    "    elif i.isupper():\n",
    "        u.append(i)\n",
    "    elif int(i)%2==0:\n",
    "        eve.append(i)\n",
    "    else:\n",
    "        od.append(i)\n",
    "A=l+u+od+eve\n",
    "st=\"\"\n",
    "for i in A:\n",
    "    st+=i\n",
    "print(st)"
   ]
