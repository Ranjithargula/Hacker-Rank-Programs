    "#12 hour format to Military Hour Format\n",
    "s='7:32:21pm'\n",
    "s=str(s)\n",
    "s=s.upper()\n",
    "l=s.split(':')\n",
    "h=int(l[0])\n",
    "if 'PM' in s:\n",
    "    h=h+12\n",
    "    s=s.replace(l[0],str(h))\n",
    "    s=s[:-2]\n",
    "    print(s)\n",
    "if 'AM'  in s:\n",
    "    s=s[:-2]\n",
    "    print(s)"
   ]
  }
