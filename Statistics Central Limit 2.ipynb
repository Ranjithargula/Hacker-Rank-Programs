   "source": [
    "import math\n",
    "EM=float(input())\n",
    "n=int(input())\n",
    "mu=float(input())\n",
    "sigma=float(input())\n",
    "m=n*mu\n",
    "s=sigma*math.sqrt(n)\n",
    "cdf=lambda x: 0.5 * (1 + math.erf((x - m) / (s * (2 ** 0.5))))\n",
    "print('{:.4f}'.format(cdf(EM)))"
   ]
  }
